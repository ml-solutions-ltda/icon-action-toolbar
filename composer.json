{
  "name": "mlsolutions/icon-action-toolbar",
  "description": "Allows you to update a single column of a resource all at once.",
  "keywords": [
    "laravel",
    "nova",
    "batch-edit",
    "toolbar"
  ],
  "authors": [],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "laravel/nova": "^5.0",
    "nova-kit/nova-packages-tool": "^2.0.2"
  },
  "autoload": {
    "psr-4": {
      "MlSolutions\\IconActionToolbar\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MlSolutions\\IconActionToolbar\\IconActionToolbarServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ]
}
